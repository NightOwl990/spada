<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/calendar_nursing"
    android:layout_width="fill_parent"
    android:layout_height="@dimen/length_100"
    android:layout_marginLeft="@dimen/d_6"
    android:layout_marginRight="@dimen/d_6"
    android:visibility="gone">

    <ImageView
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:background="@drawable/expire_item_bg" />

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_centerVertical="true"
        android:orientation="vertical">

        <TextView
            android:id="@+id/calendar_nursing_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:paddingLeft="16.0dip"
            android:paddingRight="16.0dip"
            android:text=""
            android:textColor="@color/color_333333"
            android:textSize="16.0dip" />

        <TextView
            android:id="@+id/calendar_nursing_content2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="8.0dip"
            android:paddingLeft="16.0dip"
            android:paddingRight="16.0dip"
            android:text=""
            android:textColor="@color/color_333333"
            android:textSize="13.0dip" />

        <TextView
            android:id="@+id/calendar_nursing_content1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="2.0dip"
            android:paddingLeft="16.0dip"
            android:paddingRight="16.0dip"
            android:text=""
            android:textColor="@color/color_333333"
            android:textSize="13.0dip" />
    </LinearLayout>
</RelativeLayout>